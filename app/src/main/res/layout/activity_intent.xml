<?xml version="1.0" encoding="utf-8"?>
<layout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".IntentActivity">
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <Button
                android:text="전화"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_call" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
        <Button
                android:text="구글검색"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_search" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/btn_call"/>
        <Button
                android:text="구글맵(서울 검색)"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_seoulMap" android:layout_marginEnd="148dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="148dp"
                android:layout_marginTop="8dp"
                app:layout_constraintHorizontal_bias="0.487" app:layout_constraintTop_toBottomOf="@+id/btn_search"/>
        <Button
                android:text="구글맵(경도,위도)"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btn_map" android:layout_marginEnd="148dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="148dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/btn_seoulMap"/>
    </android.support.constraint.ConstraintLayout>
</layout>